# Backend API Service Configuration

# Application
PLANT_APP_NAME=Plant Automation API
PLANT_SESSION_SECRET_KEY=your-secret-key-here-change-in-production

# Database
PLANT_DATABASE_URL=sqlite+aiosqlite:///data/plant.db

# Redis
PLANT_REDIS_URL=redis://redis:6379/0

# Admin Users (comma-separated emails)
PLANT_ADMIN_EMAILS=admin@example.com

# Telegram Bot (optional)
PLANT_TELEGRAM_BOT_TOKEN=

# JWT Settings
PLANT_JWT_SECRET_KEY=your-jwt-secret-here
PLANT_JWT_ALGORITHM=HS256
PLANT_ACCESS_TOKEN_EXPIRE_MINUTES=30
